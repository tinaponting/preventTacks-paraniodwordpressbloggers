RewriteEngine On
RewriteCond %{HTTP_USER_AGENT} "msnbot|Googlebot|bingbot|Slurp|ScoutJet|MJ12bot|Baiduspider|Ezooms|YandexBot|Exabot"
RewriteCond %{QUERY_STRING} ^replytocom=\d+$
