RewriteEngine On
# stop hotlinking
RewriteCond %{HTTP_REFERER} !^$
RewriteCond %{HTTP_REFERER} !^http(s)?://(www\.)?mysite\.com/ [NC]
allow myspace and facebook
RewriteCond %{HTTP_REFERER} !^http://(www\.)?myspace\.com [NC]
RewriteCond %{HTTP_REFERER} !^http://(www\.)?facebook\.com [NC]
